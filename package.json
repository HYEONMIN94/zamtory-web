{
  "name": "zamtory-web",
  "version": "0.1.0",
  "private": true,
  "packageManager": "yarn@1.22.22",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev": "turbo run dev",
    "dev:editor": "turbo run dev --filter=@zamtory/editor",
    "dev:zamtory": "turbo run dev --filter=@zamtory/zamtory",
    "dev:admin": "turbo run dev --filter=@zamtory/admin",
    "build": "turbo run build",
    "build:editor": "turbo run build --filter=@zamtory/editor",
    "build:zamtory": "turbo run build --filter=@zamtory/zamtory",
    "build:admin": "turbo run build --filter=@zamtory/admin",
    "lint": "turbo run lint",
    "type-check": "turbo run type-check",
    "clean": "turbo run clean && rm -rf node_modules"
  },
  "devDependencies": {
    "turbo": "^2.5.8"
  }
}
